<h2>Studios:</h2>
<% @studios.each do |studio| %>
  <div id="studio_id-<%= studio.id %>">
  <h3><%= studio.name %></h3>
  <% studio.movies.each do |movie| %>
    <div id="movie_id-<%= movie.id %>">
    <h4><%= movie.title %></h4>
    <h4><%= movie.creation_year %></h4>
    <h4><%= movie.genre %></h4>
    </div>
  <% end %>
  </div>
<% end %>